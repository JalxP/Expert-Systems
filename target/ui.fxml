         <children>
            <Label alignment="CENTER" text="Resultado:" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
            <Label fx:id="labelResults" text="A aguardar..." />
         </children>
      </HBox>
