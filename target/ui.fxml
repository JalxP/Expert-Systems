            <MenuItem mnemonicParsing="false" text="Save" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
      <Label fx:id="labelInfo" text="Preencha os dados do Paciente" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <GridPane alignment="TOP_CENTER" nodeOrientation="LEFT_TO_RIGHT">
        <columnConstraints>
          <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="178.0" minWidth="10.0" prefWidth="153.3333282470703" />
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="135.0" minWidth="0.0" prefWidth="119.66667175292969" />
            <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="237.0" minWidth="0.0" prefWidth="208.0" />
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="310.0" minWidth="10.0" prefWidth="87.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="labelName" alignment="CENTER" text="Nome" />
            <TextField fx:id="textFieldName" style="-fx-border-color: #000000; -fx-border-radius: 3;" GridPane.columnIndex="1" />
            <Label fx:id="labelAge" text="Idade" GridPane.rowIndex="1" />
            <Label fx:id="labelOMS" text="Identificador OMS" GridPane.rowIndex="5" />
            <TextField fx:id="textFieldAge" style="-fx-border-color: #000000; -fx-border-radius: 3;" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="textFieldOMS" style="-fx-border-color: #000000; -fx-border-radius: 3;" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <Label fx:id="labelCarcinomaEspinocelular" text="Carcinoma Espinocelular" GridPane.rowIndex="6" />
            <Label fx:id="labelCarcinomaIndiferenciado" text="Carcinoma Indiferenciado" GridPane.rowIndex="7" />
            <Label fx:id="labelAdenoSiewertI" text="Adenocarcinoma (Siewert I)" GridPane.rowIndex="8" />
            <Label fx:id="labelAdenoSiewertII" text="Adenocarcinoma (Siewert II)" GridPane.rowIndex="9" />
            <Label fx:id="labelGender" text="Género" GridPane.rowIndex="2" />
            <Label fx:id="labelLactant" text="Lactante" GridPane.rowIndex="3" />
            <CheckBox fx:id="checkBoxLactant" mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="3" />
